{
  "author": {
    "name": "Pierre Inglebert",
    "email": "pierre.inglebert@gmail.com"
  },
  "name": "node-zopfli",
  "description": "Bindings for Zopfli compressing lib. Compress gzip files 5% better than gzip.",
  "contributors": [
    {
      "name": "duralog",
      "email": "funisher@gmail.com"
    }
  ],
  "keywords": [
    "zopfli",
    "zlib",
    "compress",
    "gzip",
    "deflate"
  ],
  "version": "1.2.0",
  "homepage": "https://github.com/pierreinglebert/node-zopfli",
  "repository": {
    "type": "git",
    "url": "https://github.com/pierreinglebert/node-zopfli.git"
  },
  "engines": {
    "node": ">=0.10"
  },
  "binary": {
    "module_name": "zopfli",
    "module_path": "./lib/binding/{node_abi}-{platform}-{arch}",
    "remote_path": "./{configuration}",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
    "host": "https://node-zopfli.s3.amazonaws.com"
  },
  "dependencies": {
    "async": "~0.9.0",
    "bluebird": "^2.1.1",
    "buffer-crc32": "~0.2.1",
    "commander": "~2.2.0",
    "lodash": "~2.4.1",
    "nan": "~1.2.0",
    "node-pre-gyp": "~0.5.11"
  },
  "devDependencies": {
    "blanket": "~1.1.5",
    "chai": "~1.9.0",
    "coveralls": "~2.10.0",
    "jshint": "~2.5.0",
    "mocha": "~1.19.0",
    "mocha-lcov-reporter": "0.0.1",
    "randomstring": "^1.0.3"
  },
  "bin": {
    "zopfli": "bin/zopfli",
    "zopflipng": "bin/zopflipng"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build --build-from-source",
    "test": "make test"
  },
  "config": {
    "blanket": {
      "pattern": "lib",
      "data-cover-never": "node_modules"
    }
  },
  "main": "lib/zopfli.js",
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/pierreinglebert/node-zopfli/issues"
  },
